package com.example.crimeplatformapi.dto;

import java.math.BigDecimal;

public record ReportRequest(
        String caseType,           // 事件类型
        String description,        // 基本情况描述
        String locationAddress,    // 地址
        String district,           // 行政区
        BigDecimal longitude,      // 经度
        BigDecimal latitude       // 纬度
) {}

